{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src"
  ],
  "entryPointStrategy": "expand",
  "out": "./docs",
  "plugin": [
    "typedoc-theme-hierarchy"
  ],
  "theme": "hierarchy",
  "tsconfig": "./tsconfig-base.json",
  "exclude": ["**/node_modules/**/*"],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true
}